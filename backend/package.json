{
  "name": "x402-starter-kit",
  "version": "0.0.1",
  "description": "Starter kit for building paid APIs using the x402 payment protocol",
  "main": "dist/server.js",
  "type": "module",
  "bin": {
    "create-x402": "./cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc && node dist/server.js",
    "facilitator": "tsc && node dist/facilitator.js",
    "facilitator:dev": "tsc && node dist/facilitator.js",
    "test": "npm run build && node dist/testClient.js",
    "test:shell": "bash test-request.sh",
    "clean": "rm -rf dist",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate"
  },
  "keywords": [
    "x402",
    "payment-api",
    "payments",
    "usdc",
    "eip-3009"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "arweave": "^1.15.7",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "openai": "^4.78.0",
    "x402": "^0.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.2",
    "prisma": "^5.22.0",
    "typescript": "^5.9.3"
  }
}
